        "img-loader": "^4.0.0",
        "ip": "^1.1.5",
        "node-polyfill-webpack-plugin": "^1.1.4",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.3.2",
        "terser-webpack-plugin": "^5.3.10",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tslib": "^2.3.0",
        "tslint": "^6.1.3",
        "typescript": "^5.3.3",
        "typescript-tslint-plugin": "^1.0.1",
        "uglify-js": "^3.13.10",
